generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model inside_measure {
  id      Int      @id @default(autoincrement())
  inside_temperature   Float
  inside_humidity    Float
  inside_air_pressure    Float
  inside_measure_date   DateTime
  inside_measure_palce   String
}

model outside_measure {
  id      Int      @id @default(autoincrement())
  outside_temperature   Float
  outside_humidity    Float
  outside_air_pressure    Float
  outside_measure_date   DateTime
}

model threshold {
  id      Int      @id 
  outside_temperature_max_threshold   Float
  outside_temperature_min_threshold   Float
  outside_humidity_max_threshold    Float
  outside_humidity_min_threshold    Float
  outside_air_pressure_max_threshold    Float
  outside_air_pressure_min_threshold    Float
  outside_measure_date   DateTime
}
